{
  "name": "@patientenakte/zpa-portal-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && echo 'dev' > src/scripts/build.txt",
    "build:prod": "ng build --configuration production && echo 'prod' > src/scripts/build.txt",
    "prebuild:webcomponent": "echo i ran before",
    "build:webcomponent" : "ng build --configuration webcomponent && echo 'wc' > src/scripts/build.txt",
    "postbuild:webcomponent": "node src/scripts/postbuild.js",
    "watch": "ng build --watch --configuration development",
    "test": "jest --detect-open-handles",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint"
  },
  "dependencies": {
