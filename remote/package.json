    "@angular-eslint/template-parser": "^16.0.3",
    "@angular/cli": "^16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "^8.8.0",
    "jest": "^29.6.0",
    "jest-preset-angular": "13.1.1",
    "typescript": "^5.1.6"
