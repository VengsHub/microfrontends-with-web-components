{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 4,
    "generator-cli": {
        "version": "6.5.0",
        "generators": {
            "v1.0": {
                "inputSpec": "generated/resources/hcp/openapi-health-portal.yaml",
                "output": "generated/openapi",
                "generatorName": "typescript-angular",
                "additionalProperties": {
                    "supportsES6": "true",
                    "ngVersion": "15.2.6",
                    "stringEnums": "true"
                },
                "type-mappings": {
                    "set" : "Array"
                }
            }
        }
    }
}
